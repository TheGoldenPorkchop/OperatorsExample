Module OperatorsExample

    Sub Main()
        'sum operator
        Console.Write("5 + 3 = ") 'prints a string
        Console.WriteLine(5 + 3) 'prints the sum
        Console.WriteLine()

        'difference operator
        Console.Write("5 - 3 = ") 'prints a string
        Console.WriteLine(5 - 3) 'prints the sum
        Console.WriteLine()
        'product operator
        Console.Write("5 * 3 = ") 'prints a string
        Console.WriteLine(5 * 3) 'prints the sum
        Console.WriteLine()

        'quotient operator
        Console.Write("5 / 3 = ") 'prints a string
        Console.WriteLine(5 / 3 * 3) 'prints the sum
        Console.WriteLine()
    End Sub

End Module
